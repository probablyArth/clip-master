generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id      String   @id @default(uuid())
  name    String
  api_key String?  @unique
  videos  Videos[]
  limit   Int
}

model Videos {
  id            String @id @default(uuid())
  name          String
  original_name String
  size          BigInt
  path          String @unique
  duration      Int
  user          Users  @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String
}
